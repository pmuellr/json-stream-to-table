{
  "name": "json-stream-to-table",
  "version": "0.0.1",
  "description": "convert a stream of JSON lines to tabular format",
  "main": "json-stream-to-table",
  "license": "MIT",
  "author": "Patrick Mueller <pmuellr@apache.org> (https://github.com/pmuellr)",
  "keywords": [
    "json",
    "table"
  ],
  "bin": {
    "json-stream-to-table": "json-stream-to-table.js"
  },
  "scripts": {
    "standard": "standard",
    "watch": "nodemon --exec 'npm run standard' --ext js,json"
  },
  "dependencies": {
    "lodash": "~4.13.1"
  },
  "devDependencies": {
    "nodemon": "~1.9.1",
    "standard": "~6.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pmuellr/json-stream-to-table.git"
  },
  "bugs": {
    "url": "https://github.com/pmuellr/json-stream-to-table/issues"
  },
  "homepage": "https://github.com/pmuellr/json-stream-to-table",
  "dependencies": {
    "yargs": "~4.4.0"
  }
}
